{
  "name": "personal-finance-app",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "seed": "tsx prisma/seed.ts",
    "setup": "npm install && npx prisma db push && npm run seed"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "dotenv": "^16.4.7",
    "prettier": "^3.6.2",
    "supabase": "^2.67.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "5.16.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.56.1",
    "@tanstack/react-query": "^5.85.6",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "lucide-react": "^0.542.0",
    "next-intl": "^4.6.0",
    "postcss": "^8.5.6",
    "prisma": "5.16.2",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5"
  }
}
